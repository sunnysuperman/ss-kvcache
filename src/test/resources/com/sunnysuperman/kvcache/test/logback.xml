<?xml version="1.0"?>
<configuration scan="false">
	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%level] %d{HH:mm:ss.SSS} [%thread] - %msg%n
			</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="console" />
	</root>
</configuration>